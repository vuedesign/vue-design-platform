{
    "name": "vue.design",
    "description": "Build an vue.design Extension!",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "/assets/scripts/background.js"
    },
    "permissions": ["storage", "activeTab", "scripting", "tabs", "cookies"],
    "host_permissions": ["http://localhost:3004/"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "/assets/images/logo_16.png",
            "32": "/assets/images/logo_32.png",
            "48": "/assets/images/logo_48.png",
            "128": "/assets/images/logo_64.png"
        }
    },
    "icons": {
        "16": "/assets/images/logo_16.png",
        "32": "/assets/images/logo_32.png",
        "48": "/assets/images/logo_48.png",
        "128": "/assets/images/logo_64.png"
    },
    "options_page": "options.html",
    "externally_connectable": {
        "matches": ["http://localhost:3004/api/*", "https://t7.baidu.com/*"]
    }
}
